<h1>進捗一覧</h1>
<%= link_to "進捗登録", new_task_read_path(task) %>
<table>
  <thead>
    <tr>
      <th scope="col">No.</th>
      <th scope="col">読んだ日</th>
      <th scope="col">読んだページ数</th>
    </tr>
  </thead>
  <tbody>
    <%= render partial: "reads/read_data", locals: { task: task }, collection: reads, as: "read" %>
  </tbody>
</table>